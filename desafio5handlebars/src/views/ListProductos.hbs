  
{{#if productos}}

    <div class="row">
        <table Class="table">
            <thead>
               
                    <th>Title</th>
                    <th>Price</th>
                    <th>Thumbnail</th>
               
            </thead>
            <tbody >
                {{#each productos}}
                <tr>
                    <td>{{this.title}}</td>
                    <td>{{this.price}}</td>
                    <td>{{this.thumbnail}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
    <br><br>
{{else}}
    <h3 style="blackground-color: red;">No hay Producto</h3>
    <br><br>
{{/if}}


     <div class="row">
        <div class=" offset-1" >
            <a  class="btn btn-warning" href="../"> <i class="fa fa-solid fa-reply " ></i> &nbsp; Ingresar Producto</a>
        </div>
    </div>


  
   